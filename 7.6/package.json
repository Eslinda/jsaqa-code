{
  "name": "cypress-test",
  "version": "1.0.0",
  "description": "first tests with Cypress",
  "main": "index.js",
  "scripts": {
    "chrome:headed:notebooks": "cypress run --headed --browser chrome",
    "chrome:headed:mobile": "cypress run --config-file mobile.config.js --headed --browser chrome",  
    "chrome:headless:notebooks": "cypress run --browser chrome",
    "chrome:headless:mobile": "cypress run --config-file mobile.config.js --browser chrome",
    "edge:headed:notebooks": "cypress run --headed --browser edge",
    "edge:headed:mobile": "cypress run --config-file mobile.config.js --headed --browser edge",
    "edge:headless:notebooks": "cypress run --browser edge",
    "edge:headless:mobile": "cypress run --config-file mobile.config.js --browser edge",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "OM",
  "license": "ISC",
  "dependencies": {
    "cypress": "^12.12.0"
  }
}
